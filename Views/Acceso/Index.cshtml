@model CubiculosTEC.Models.Acesso

<!DOCTYPE html>
<html lang="en">
<head>
	<title>Login CubiculosTEC</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
    <style type="text/css">
        body {font-family: Arial;font-size: 10pt;}
        .error {color: red;}
        .aa { background-color:#66546; border-radius: 20px 20px 20px 20px }
    </style>
</head>
<body>
    @using (Html.BeginForm("Index","Acceso", FormMethod.Post)){
            <table>
                <tr>
                    <td>Correo institucional <br> @Html.TextBoxFor(m => m.correo, new {@class="aa"}) <br> </td>
                    <td>@Html.ValidationMessageFor(m => m.correo,"", new {@class="error"})</td>
                </tr>
                <tr>
                    <td>Contrase√±a <br> @Html.PasswordFor(m => m.contrasena) <br> </td>
                    <td>@Html.ValidationMessageFor(m => m.contrasena,"", new {@class="error", @type="password"})</td>
                    <td>@Html.ValidationMessage("Custom Error", new {@class="error"})</td>
                </tr>
                <tr></tr>                
                <tr>
                    <td><button type="submit"> Ingresar</button></td>
                    <td></td>
                </tr>
            </table>


    }
</body>
</html>